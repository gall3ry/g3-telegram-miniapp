{
  "name": "database-schema",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/database-schema/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "packages/database-schema"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "packages/database-schema"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "packages/database-schema"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/database-schema/**/*.ts"]
      }
    }
  }
}
