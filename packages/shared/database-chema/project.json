{
  "name": "shared-database-chema",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/database-chema/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "packages/shared/database-chema"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "packages/shared/database-chema"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "packages/shared/database-chema"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shared/database-chema/**/*.ts"]
      }
    }
  }
}
