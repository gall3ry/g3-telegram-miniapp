{
  "name": "@g3-backend/trpc-server",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/g3-backend/trpc-server/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "format": ["esm", "cjs"],
        "outputPath": "dist/packages/g3-backend/trpc-server",
        "tsConfig": "packages/g3-backend/trpc-server/tsconfig.lib.json",
        "main": "packages/g3-backend/trpc-server/src/index.ts",
        "assets": [],
        "generateExportsField": true
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
